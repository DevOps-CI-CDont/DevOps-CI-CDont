{
  "extends": ["eslint:recommended", "next/core-web-vitals"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint/eslint-plugin"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": [
      "error",
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
